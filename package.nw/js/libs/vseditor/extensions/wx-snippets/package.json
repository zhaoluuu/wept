{
  "name": "wx-snippets",
  "displayName": "WX Snippets",
  "description": "Snippets for WeChat MiniProgram",
  "version": "2.11.0",
  "publisher": "wechat.miniprogram",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [{
      "language": "javascript",
      "path": "./snippets/snippets.js.json"
    }, {
      "language": "typescript",
      "path": "./snippets/snippets.ts.json"
    }, {
      "language": "json",
      "path": "./snippets/snippets.json.json"
    }]
  },
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "main.js"
}